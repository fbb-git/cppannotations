#!/bin/sh

cd ../yo

GIFS=`find ./ -name '*.gif'`

if [ "$GIFS" == "" ] ; then
    echo no gifs
else
    tar cf - $GIFS | (cd ../html; tar xvf -)
    rm $GIFS
fi


PS=`find ./ -name '*.ps'`

if [ "$PS" == "" ] ; then
    echo no ps files
else
    tar cf - $PS | (cd ../latex; tar xvf -)
    rm $PS
fi


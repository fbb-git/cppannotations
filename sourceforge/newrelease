#!/bin/bash

cd ..
./build clean
./build svnclean
./build programs

cd bin
./createhtml
./ziphtml

cd ../sourceforge
yodl2html index || exit 1
unzip ../zip/cplusplus.html.zip
mv annotations cppannotations

scp -r index.html cppannotations \
        fbb,cppannotations@web.sourceforge.net:htdocs/

cd ..
./build clean
./build svnclean

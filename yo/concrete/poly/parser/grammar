%class-name Parser

%filenames parser
%parsefun-source parse.cc
%scanner ../scanner/scanner.h
// %debug

%baseclass-preinclude      preinclude.h

%stype   Semantic

%token INT IDENTIFIER 

%%

rules:
    rules rule
|
    rule
;

rule:
    IDENTIFIER '(' IDENTIFIER ')' ';'
    {
          cout << $1 << " " << $3 << '\n';
    }
|
    IDENTIFIER '=' INT ';'
    {
          cout << $1 << " " << $3 << '\n';
    }
;
